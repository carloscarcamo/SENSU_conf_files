{
  "rabbitmq": {
    "host": "<rabbit_server>",
    "port": 5671,
    "vhost": "/sensu",
    "user": "sensu",
    "password": "<password>",
    "heartbeat": 30,
    "prefetch": 50,
    "ssl": {
      "cert_chain_file": "/etc/sensu/ssl/cert.pem",
      "private_key_file": "/etc/sensu/ssl/key.pem"
    }
  }
}
